# Generated reports
reports/*
!reports/.gitkeep

# Database
*.db
*.sqlite
*.sqlite3

# Logs
*.log
logs/

# Test coverage
.coverage
htmlcov/
.pytest_cache/

# Distribution
dist/
build/
*.egg-info/

# OS files
.DS_Store
Thumbs.db
